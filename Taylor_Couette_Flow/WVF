# Taylor-Coeutte flow
# Initial perturbatuion to get symmetric TVF was
#	w = (A*y+B/y)*(1.0-0.5*exp(-8.0*x^2))
#
# Perturbation to get WVF to be added to TVF flow is
#	u = epsi*0.5*lamb*sin(PI*(y-Rc))*cos(BETA*z)*sin(2*PI*x/lamb)/y
#	v = 2.0*epsi*cos(PI*(y-Rc))*cos(BETA*z)*cos(2*PI*x/lamb)/y
#	w = epsi*PI*sin(PI*(y-Rc))*sin(BETA*z)*cos(2*PI*x/lamb)/BETA



<FIELDS>
	u v w p
</FIELDS>

<USER>
	u = 0
	v = 0
	w = 0
	p = 0
</USER>

<TOKENS>
        CYLINDRICAL = 1
	N_P       = 10
	N_Z       = 32
	BETA      = 12
	D_T       = 0.001
	TOL_REL   = 1.0e-8
	N_TIME    = 2
	N_STEP    = 1000
	AVERAGE   = 0
	CHKPOINT  = 1
	ITERATIVE = 0
	IO_HIS    = 5
	IO_CFL    = 10
	IO_FLD    = 5000
	Reynolds  = 410
	KINVIS    = 1.0/Reynolds
	Re_I	  = 1370
	Re_O	  = 960
	Ri        = 7.54701
	Ro        = 8.54701
	Rc 	  = 0.5*(Ri+Ro)
	omega	  = 0.3183
	OMi       = Re_I/Reynolds/Ri-omega
	OMo       = Re_O/Reynolds/Ro-omega
	A         = (OMi*Ri*Ri-OMo*Ro*Ro)/(Ri*Ri-Ro*Ro)
	B         = Ri*Ri*Ro*Ro*(OMo-OMi)/(Ri*Ri-Ro*Ro)
	epsi      = 0.005
	lamb	  = 2.00
</TOKENS>

<FORCE>
	CORIOLIS_UNSTEADY = 0
	CORIOLIS_OMEGA_X = omega
	CORIOLIS_OMEGA_Y = 0
	CORIOLIS_OMEGA_Z = 0
</FORCE>

<GROUPS NUMBER=2>
	1	i	inner
	2	w	wall
</GROUPS>

<BCS NUMBER=2>
	1	i	4
			<D>	u = 0		</D>
			<D>	v = 0		</D>
			<D>     w = A*y+B/y	</D>
			<H>	p = 0		</H>
	2	w	4
			<D>	u = 0		</D>
			<D>	v = 0		</D>
			<D>     w = A*y+B/y	</D>
			<H>	p = 0		</H>
</BCS>

<HISTORY NUMBER=9>
